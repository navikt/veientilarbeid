{
    "name": "veientilarbeid",
    "version": "0.1.0",
    "type": "module",
    "private": true,
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "build:demo": "vite build --mode demo",
        "build:cdn": "vite build --mode cdn",
        "lint": "eslint --ext=jsx,js,ts,tsx ./src",
        "lint:fix": "eslint --ext=jsx,js,ts,tsx ./src --fix",
        "test": "npm run lint && cross-env NODE_OPTIONS=--no-experimental-fetch vitest dev",
        "test-ci": "npm run lint && cross-env NODE_OPTIONS=--no-experimental-fetch CI=true vitest run",
        "test:coverage": "vitest run --coverage",
        "deploy": "npm run build && gh-pages -d build",
        "prettier": "prettier --write 'src/**/*.ts' 'src/**/*.tsx' 'src/**/*.css'",
        "refresh": "rm -rf node_modules && rm package-lock.json && npm i",
        "snyk-protect": "snyk-protect",
        "prepare": "npm run snyk-protect",
        "postinstall": "husky install"
    },
    "dependencies": {
        "@alheimsins/uker-fra-dato": "1.0.4",
        "@alheimsins/virkedager": "2.2.0",
        "@navikt/ds-css": "2.8.1",
        "@navikt/ds-icons": "2.8.1",
        "@navikt/ds-react": "2.8.1",
        "@snyk/protect": "1.1121.0",
        "amplitude-js": "8.21.6",
        "classnames": "2.3.2",
        "core-js": "3.29.1",
        "intersection-observer": "0.12.2",
        "nanoid": "4.0.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-in-viewport": "1.0.0-alpha.29",
        "swr": "2.1.0"
    },
    "devDependencies": {
        "@rollup/plugin-terser": "0.4.0",
        "@testing-library/dom": "9.0.1",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "14.0.0",
        "@testing-library/user-event": "14.4.3",
        "@types/amplitude-js": "8.16.2",
        "@types/merge-deep": "3.0.0",
        "@types/react": "18.0.28",
        "@types/react-dom": "18.0.11",
        "@vitejs/plugin-react": "3.1.0",
        "c8": "7.13.0",
        "cross-env": "7.0.3",
        "eslint": "8.36.0",
        "eslint-config-react-app": "7.0.1",
        "husky": "8.0.3",
        "jsdom": "21.1.1",
        "lint-staged": "13.2.0",
        "merge-deep": "3.0.3",
        "msw": "1.1.0",
        "prettier": "2.8.4",
        "rollup-plugin-import-map": "3.0.0",
        "typescript": "4.9.5",
        "vite": "4.2.0",
        "vite-plugin-css-injected-by-js": "3.1.0",
        "vite-plugin-environment": "1.1.3",
        "vite-plugin-html": "3.2.0",
        "vite-plugin-svgr": "2.4.0",
        "vitest": "0.29.3",
        "whatwg-fetch": "3.6.2"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie < 11",
        "not op_mini all"
    ],
    "prettier": {
        "printWidth": 120,
        "singleQuote": true,
        "tabWidth": 4
    },
    "lint-staged": {
        "*.{js,jsx,json,md,ts,tsx}": "prettier --write"
    },
    "snyk": true,
    "msw": {
        "workerDirectory": "public"
    },
    "engines": {
        "node": ">=18.15.0"
    }
}
