{
    "name": "veientilarbeid",
    "version": "0.1.0",
    "type": "module",
    "private": true,
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "build:demo": "vite build --mode demo",
        "build:cdn": "vite build --mode cdn",
        "lint": "eslint --ext=jsx,js,ts,tsx ./src",
        "lint:fix": "eslint --ext=jsx,js,ts,tsx ./src --fix",
        "test": "npm run lint && cross-env NODE_OPTIONS=--no-experimental-fetch vitest dev",
        "test-ci": "npm run lint && cross-env NODE_OPTIONS=--no-experimental-fetch CI=true vitest run",
        "test:coverage": "vitest run --coverage",
        "deploy": "npm run build && gh-pages -d build",
        "prettier": "prettier --write 'src/**/*.ts' 'src/**/*.tsx' 'src/**/*.css'",
        "refresh": "rm -rf node_modules && rm package-lock.json && npm i",
        "prepare": "husky install"
    },
    "dependencies": {
        "@alheimsins/uker-fra-dato": "1.0.4",
        "@alheimsins/virkedager": "2.2.0",
        "@grafana/faro-web-sdk": "1.1.4",
        "@grafana/faro-web-tracing": "1.1.4",
        "@navikt/aksel-icons": "5.3.1",
        "@navikt/ds-css": "5.3.1",
        "@navikt/ds-react": "5.3.1",
        "amplitude-js": "8.21.9",
        "classnames": "2.3.2",
        "core-js": "3.32.1",
        "intersection-observer": "0.12.2",
        "nanoid": "4.0.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-in-viewport": "1.0.0-alpha.30",
        "swr": "2.2.2"
    },
    "devDependencies": {
        "@rollup/plugin-terser": "0.4.3",
        "@testing-library/dom": "9.3.1",
        "@testing-library/jest-dom": "6.1.2",
        "@testing-library/react": "14.0.0",
        "@testing-library/user-event": "14.4.3",
        "@types/amplitude-js": "8.16.2",
        "@types/merge-deep": "3.0.0",
        "@types/react": "18.2.21",
        "@types/react-dom": "^18.2.7",
        "@vitejs/plugin-react": "4.0.4",
        "c8": "8.0.1",
        "cross-env": "7.0.3",
        "eslint": "8.48.0",
        "eslint-config-react-app": "7.0.1",
        "husky": "8.0.3",
        "jsdom": "22.1.0",
        "lint-staged": "^14.0.1",
        "merge-deep": "3.0.3",
        "msw": "1.3.0",
        "prettier": "3.0.3",
        "rollup-plugin-import-map": "3.0.0",
        "typescript": "5.2.2",
        "vite": "4.4.9",
        "vite-plugin-css-injected-by-js": "^3.3.0",
        "vite-plugin-environment": "1.1.3",
        "vite-plugin-html": "3.2.0",
        "vite-plugin-svgr": "3.2.0",
        "vitest": "0.34.3",
        "whatwg-fetch": "3.6.18"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie < 11",
        "not op_mini all"
    ],
    "prettier": {
        "printWidth": 120,
        "singleQuote": true,
        "tabWidth": 4
    },
    "lint-staged": {
        "*.{js,jsx,json,md,ts,tsx}": "prettier --write"
    },
    "snyk": true,
    "msw": {
        "workerDirectory": "public"
    },
    "engines": {
        "node": ">=18.17.1"
    }
}
