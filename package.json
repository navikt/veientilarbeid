{
    "name": "veientilarbeid",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "cross-env REACT_APP_MOCK=true node scripts/start.js",
        "start:esm": "cross-env REACT_APP_MOCK=true vite",
        "build": "REACT_APP_SENTRY_RELEASE=$(sentry-cli releases propose-version) node scripts/build.js",
        "build:micro": "cross-env REACT_APP_MICRO=true npm run build",
        "build:esm": "vite build",
        "build:esm-demo": "vite build --mode demo",
        "lint": "eslint --ext=jsx,js,ts,tsx ./src",
        "lint:fix": "eslint --ext=jsx,js,ts,tsx ./src --fix",
        "test": "npm run lint && vitest dev",
        "test-ci": "npm run lint && CI=true vitest run",
        "test:coverage": "vitest run --coverage",
        "deploy": "npm run build && gh-pages -d build",
        "prettier": "prettier --write 'src/**/*.ts' 'src/**/*.tsx' 'src/**/*.css'",
        "refresh": "rm -rf node_modules && rm package-lock.json && npm i",
        "vercel-build": "cross-env REACT_APP_MOCK=true npm run build",
        "snyk-protect": "snyk-protect",
        "prepare": "npm run snyk-protect",
        "sentry-release": "SENTRY_RELEASE=$(sentry-cli releases propose-version) node scripts/sentry-release",
        "postinstall": "husky install"
    },
    "dependencies": {
        "@alheimsins/uker-fra-dato": "1.0.4",
        "@alheimsins/virkedager": "2.2.0",
        "@navikt/ds-css": "0.18.19",
        "@navikt/ds-icons": "0.8.19",
        "@navikt/ds-react": "0.19.15",
        "@sentry/browser": "6.19.7",
        "@sentry/react": "6.19.7",
        "@snyk/protect": "1.982.0",
        "amplitude-js": "8.18.5",
        "classnames": "2.3.1",
        "core-js": "3.24.1",
        "intersection-observer": "0.12.2",
        "nanoid": "3.3.4",
        "react": "17.0.2",
        "react-app-polyfill": "3.0.0",
        "react-dom": "17.0.2",
        "react-in-viewport": "1.0.0-alpha.20",
        "vite-plugin-html": "3.2.0",
        "vite-plugin-svgr": "2.2.0"
    },
    "devDependencies": {
        "@sentry/cli": "1.74.2",
        "@testing-library/dom": "8.12.0",
        "@testing-library/jest-dom": "5.16.3",
        "@testing-library/react": "12.1.4",
        "@testing-library/user-event": "13.5.0",
        "@types/amplitude-js": "8.16.0",
        "@types/merge-deep": "3.0.0",
        "@types/react": "17.0.43",
        "@types/react-dom": "17.0.14",
        "@vitejs/plugin-react": "1.3.2",
        "c8": "7.11.3",
        "cross-env": "7.0.3",
        "husky": "7.0.4",
        "jsdom": "20.0.0",
        "lint-staged": "12.3.7",
        "merge-deep": "3.0.3",
        "msw": "0.39.2",
        "prettier": "2.6.1",
        "react-scripts": "5.0.0",
        "rewire": "6.0.0",
        "rollup-plugin-import-map": "2.2.2",
        "rollup-plugin-terser": "7.0.2",
        "typescript": "4.5.5",
        "vite": "3.0.4",
        "vite-plugin-css-injected-by-js": "1.5.1",
        "vitest": "0.20.2"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie < 11",
        "not op_mini all"
    ],
    "prettier": {
        "printWidth": 120,
        "singleQuote": true,
        "tabWidth": 4
    },
    "lint-staged": {
        "*.{js,jsx,json,md,ts,tsx}": "prettier --write"
    },
    "snyk": true,
    "msw": {
        "workerDirectory": "public"
    },
    "engines": {
        "node": ">=16.16.0"
    }
}
