  "private": true,
  "scripts": {
    "start": "npm-run-all -p start:*",
    "start:ts": "cross-env REACT_APP_MOCK=true craco start",
    "start:tekster": "node ./scripts/watch-tekster.js",
    "build": "npm-run-all -s build:tekster build:ts",
    "build:micro": "cross-env REACT_APP_MICRO=true npm-run-all -s build:tekster build:ts",
    "build:ts": "craco build",
    "build:tekster": "react-intl-bundler --no-lookup src/tekster src/tekster",
    "lint": "tslint ./src/**/*.ts*",
    "lint:fix": "tslint ./src/**/*.ts* --fix",
    "test": "npm run lint && npm run build:tekster && craco test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "deploy": "npm run build && gh-pages -d build",
    "prettier": "prettier --write --single-quote --tab-width 4 src/**/*.ts src/**/*.tsx src/**/*.less"
  },
