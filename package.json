{
  "name": "veientilarbeid",
  "homepage": "/veientilarbeid",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all -p start:*",
    "start:ts": "cross-env REACT_APP_MOCK=true react-scripts-ts start",
    "start:tekster": "node ./scripts/watch-tekster.js",
    "build": "npm-run-all -s build:tekster build:ts",
    "build:ts": "react-scripts-ts build",
    "build:tekster": "react-intl-bundler --no-lookup src/tekster src/tekster",
    "lint": "tslint ./src/**/*.ts*",
    "lint:fix": "tslint ./src/**/*.ts* --fix",
    "test": "npm run build:tekster && react-scripts-ts test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "heroku": "npm-run-all -s heroku:build heroku:start",
    "heroku:start": "node heroku-server.js",
    "heroku:build": "npm run build:tekster && cross-env REACT_APP_MOCK=true GENERATE_SOURCEMAP=false react-scripts-ts --max_old_space_size=384 build",
    "deploy": "npm run build && gh-pages -d build",
    "eject": "react-scripts-ts eject",
    "prettier": "prettier --write --single-quote --tab-width 4 src/**/*.ts src/**/*.tsx src/**/*.less"
  },
  "dependencies": {
    "@types/classnames": "^2.2.4",
